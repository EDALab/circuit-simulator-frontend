//Font
//The default font, mainly various explanatory texts, sans-serif English
font-default = "Helvetica", "Arial", "Hiragino Sans GB", "WenQuanYi Micro Hei", "Microsoft YaHei", sans-serif
//Serif fonts, mainly used in prominent positions such as titles
font-serif = "Georgia", "Hiragino Sans GB", "WenQuanYi Micro Hei", "Microsoft YaHei", serif
//Device attribute description text
font-text = "Times New Roman", "Microsoft YaHei"

//Animetion related parameters
//Sidebar fade in and out
bezier-aside = cubic-bezier(.3,.3,.1,1)
time-aside = 500ms
//The blinking effect of various input boxes
bezier-input = cubic-bezier(0,1.5,1,1)
time-input = 300ms